############################################
# .env.example
#
# Copy this file to .env and fill in the values
############################################

# DB_NAME
# The name of your database.
# Example: "my_database"
DB_NAME="my_database"

# BUN_PORT
# The port on which your Bun server will listen.
# Example: 3000 or 8080
BUN_PORT=3000

# OPENAI_API_KEY
# Your API key for OpenAI’s services (e.g., ChatGPT, GPT-4).
# Obtain this from https://platform.openai.com/account/api-keys
OPENAI_API_KEY="sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# PINATA_JWT
# JSON Web Token for Pinata, used to authenticate and manage IPFS services.
# Obtain this from your Pinata account settings.
PINATA_JWT="eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.abcdef..."

# PINATA_GATEWAY_URL
# The base gateway URL for accessing your pinned IPFS files through Pinata.
# Typically something like: https://<your-subdomain>.mypinata.cloud/ipfs
PINATA_GATEWAY_URL="https://gateway.pinata.cloud/ipfs"

# TWITTER_API_KEY
# Your Twitter API key obtained from Twitter Developer Portal.
# Used to interact with Twitter’s API (post tweets, read feeds, etc.).
TWITTER_API_KEY="xxxxxxxxxxxxxxxxxxxxx"

# TWITTER_API_SECRET
# Your Twitter API secret key, also from the Twitter Developer Portal.
# Keep this confidential.
TWITTER_API_SECRET="xxxxxxxxxxxxxxxxxxxxx"

# CREATE_AGENT_SECRET
# A secret token or key used to sign or authenticate agent creation requests in your application.
# Use a randomly generated string. Keep this very secure.
CREATE_AGENT_SECRET="random-secret-string"

# DALLE_API_KEY
# API key for the DALL·E service, if your application needs to generate images via DALL·E.
# Obtain this similarly from the OpenAI API keys page.
DALLE_API_KEY="sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

